package libraryy;

import java.util.ArrayList;

public class ArrryyLst {
	public static void main(String[] args) {
		ArrayList al = new ArrayList();
		al.add("kapil");
		al.add(23);
		al.add(47.0f);
		System.out.println(al);
	}

}
