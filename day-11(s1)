3. package libraryy;

import java.util.LinkedList;
public class LinkedListEx {
	public static void main(String[] args) {
		LinkedList li = new LinkedList();
		li.add(123);
		li.add("kapil");
		li.add("y");
		li.add(true);
		System.out.println("size is"+li.size());
		System.out.println("list contains kapil"+li.contains("kapil"));
		System.out.println("get first element"+li.getFirst());
		System.out.println("last element is"+li.getLast());
		System.out.println("list is empty or not"+li.isEmpty());
		System.out.println("to remove"+li.remove());
		System.out.println("size is"+li.size());
		System.out.println("all elemets are :"+li);
		

}
}
