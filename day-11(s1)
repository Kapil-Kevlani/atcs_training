5.
package libraryy;
import java.util.ArrayList;

public class evennumber {
	public static void main(String[] args) {
		ArrayList al = new ArrayList();
		for(int i =0;i<10;i++) {
			al.add(i);
		}
		for(Object o:al) {
			Integer i = (Integer) o;
			if(i%2==0) {
				System.out.println(i);
			}
		}
	}

}
