package libraryy;
import java.util.LinkedList;



public class Test2 {
	public static void main(String[] args) {
		LinkedList li = new LinkedList();
		li.add("jimmy");
		li.add(34);
		li.add(true);
		li.add(32.5f);
		System.out.println("size of ll is: "+li.size());
		System.out.println("do this list contains jimmy?: "+li.contains("jimmy"));
		System.out.println("is this list empty: "+li.isEmpty());
		System.out.println("the first element is: "+li.getFirst());
	}

}
